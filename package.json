{
  "name": "datum",
  "version": "2.0.1",
  "description": "Quickly add data to couch db",
  "repository": "git@github.com:vividn/datum.git",
  "author": "Nate Faber <nathaniel@njfaber.com>",
  "license": "UNLICENSED",
  "main": "build/src/index.js",
  "bin": {
    "dtm": "./build/src/index.js",
    "eqcheck": "./build/projects/finance/src/eqcheck.js",
    "txview": "./build/projects/finance/src/transactionView.js",
    "fbal": "./build/projects/finance/src/fbal.js",
    "wordview": "./build/projects/language/src/word.js",
    "chorelist": "./build/projects/datum/src/chorelist.js"
  },
  "engines": {
    "node": ">18.0.0"
  },
  "scripts": {
    "test": "jest",
    "testwatch": "jest --watchAll",
    "type": "tsc -p tsconfig.withTests.json --skipLibCheck",
    "typewatch": "tsc -p tsconfig.withTests.json -w --skipLibCheck",
    "build": "rm -rf ./build; npx tsc -p . --skipLibCheck",
    "buildwatch": "tsc -p . -w --skipLibCheck",
    "system": "rm -rf ./node_modules && pnpm i --production && pnpm build && pnpm --global add $PWD",
    "refresh": "rm -rf ./node_modules && pnpm install",
    "lint": "eslint src",
    "fmt": "eslint src --fix",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "^3.2.5",
    "prettier-2": "npm:prettier@^2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@types/argparse": "^2.0.15",
    "@types/lodash.flatten": "^4.4.9",
    "@types/lodash.flattendeep": "^4.4.9",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.isequal": "^4.5.8",
    "@types/lodash.isplainobject": "^4.0.9",
    "@types/lodash.sample": "^4.2.9",
    "@types/lodash.set": "^4.3.9",
    "@types/lodash.unset": "^4.5.9",
    "@types/luxon": "^3.4.2",
    "@types/node": "^18.13.0",
    "@types/pluralize": "^0.0.33",
    "@types/pouchdb": "^6.4.2",
    "@types/prompt-sync": "^4.2.3",
    "@types/relaxed-json": "^1.0.4",
    "@types/shell-quote": "^1.7.5",
    "@types/tmp": "^0.2.6",
    "argparse": "^2.0.1",
    "chalk": "^4.1.2",
    "chrono-node": "^2.7.5",
    "dotenv": "^16.4.5",
    "easy-table": "^1.2.0",
    "lodash.flatten": "^4.4.0",
    "lodash.flattendeep": "^4.4.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.sample": "^4.2.1",
    "lodash.set": "^4.3.2",
    "lodash.unset": "^4.5.2",
    "luxon": "^3.4.4",
    "parse-duration": "^1.1.0",
    "picocolors": "^1.0.0",
    "pluralize": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "pouchdb": "^8.0.1",
    "pouchdb-adapter-memory": "^8.0.1",
    "printf": "^0.6.1",
