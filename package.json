{
  "name": "datum",
  "version": "2.0.1",
  "description": "Quickly add data to couch db",
  "repository": "git@github.com:vividn/datum.git",
  "author": "Nate Faber <nathaniel@njfaber.com>",
  "license": "UNLICENSED",
  "main": "build/src/index.js",
  "bin": {
    "dtm": "./build/src/index.js",
    "eqcheck": "./build/projects/finance/src/eqcheck.js",
    "txview": "./build/projects/finance/src/transactionView.js",
    "fbal": "./build/projects/finance/src/fbal.js",
    "wordview": "./build/projects/language/src/word.js"
  },
  "engines": {
    "node": ">18.0.0"
  },
  "scripts": {
    "test": "jest",
    "testwatch": "jest --watchAll",
    "type": "tsc -p tsconfig.withTests.json --skipLibCheck",
    "typewatch": "tsc -p tsconfig.withTests.json -w --skipLibCheck",
    "build": "rm -rf ./build; tsc -p . --skipLibCheck",
    "buildwatch": "tsc -p . -w --skipLibCheck",
    "system": "NODE_ENV=production yarn global add $PWD",
    "pipeline": "yarn refresh && yarn test --maxWorkers 1 && yarn build && rm -rf ./node_modules && yarn install --production && yarn system",
    "refresh": "rm -rf ./node_modules && yarn install",
    "lint": "eslint src",
    "fmt": "eslint src --fix",
    "postinstall": "yarn cache clean datum"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^27.4.1",
    "@types/lodash.flatten": "^4.4.8",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.isplainobject": "^4.0.7",
    "@types/lodash.sample": "^4.2.7",
    "@types/lodash.set": "^4.3.7",
    "@types/lodash.unset": "^4.5.7",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.13.0",
    "@types/pluralize": "^0.0.29",
    "@types/pouchdb": "^6.4.1",
    "@types/prompt-sync": "^4.2.0",
    "@types/react": "^18.2.0",
    "@types/relaxed-json": "^1.0.1",
    "@types/tmp": "^0.2.3",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^29.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^1.1.5",
    "jest": "^26.6.3",
    "jest-mock-extended": "^3.0.4",
    "prettier": "^2.8.8",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/lodash.get": "^4.4.7",
    "chalk": "^4.1.2",
    "chrono-node": "^2.7.3",
    "dotenv": "^16.0.3",
    "easy-table": "^1.2.0",
    "ink": "^3.2.0",
    "ink-table": "^3.0.0",
    "lodash.flatten": "^4.4.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.sample": "^4.2.1",
    "lodash.set": "^4.3.2",
    "lodash.unset": "^4.5.2",
    "luxon": "^3.3.0",
    "parse-duration": "^1.1.0",
    "picocolors": "^1.0.0",
    "pluralize": "^8.0.0",
    "pouchdb": "^8.0.1",
    "pouchdb-adapter-memory": "^8.0.1",
    "printf": "^0.6.1",
    "prompt-sync": "^4.2.0",
    "react": "^18.2.0",
    "relaxed-json": "^1.0.3",
    "string.ify": "^1.0.64",
    "tmp-promise": "^3.0.3",
    "yargs": "^17.7.1"
  }
}
