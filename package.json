{
  "name": "datum",
  "version": "2.0.1",
  "description": "Quickly add data to couch db",
  "repository": "git@github.com:vividn/datum.git",
  "author": "Nate Faber <nathaniel@njfaber.com>",
  "license": "UNLICENSED",
  "main": "build/src/index.js",
  "bin": {
    "dtm": "./build/src/index.js"
  },
  "scripts": {
    "test": "jest",
    "testwatch": "jest --watchAll",
    "type": "tsc -p tsconfig.withTests.json",
    "typewatch": "tsc -p tsconfig.withTests.json -w",
    "build": "rm -rf ./build; tsc -p .",
    "buildwatch": "tsc -p . -w",
    "system": "NODE_ENV=production yarn global add $PWD",
    "pipeline": "yarn refresh && yarn test --maxWorkers 1 && yarn build && rm -rf ./node_modules && yarn install --production && yarn system",
    "refresh": "rm -rf ./node_modules && yarn install",
    "lint": "prettier src --check && eslint src",
    "fmt": "prettier src --write && eslint src --fix",
    "postinstall": "yarn cache clean datum"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/lodash.set": "^4.3.6",
    "@types/lodash.unset": "^4.5.6",
    "@types/luxon": "^2.3.1",
    "@types/nano": "^7.0.0",
    "@types/node": "^14.14.37",
    "@types/pluralize": "^0.0.29",
    "@types/relaxed-json": "^1.0.1",
    "@types/tmp": "^0.2.3",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^27.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "jest": "^26.6.3",
    "jest-mock-extended": "^2.0.4",
    "prettier": "^2.6.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@types/lodash.get": "^4.4.6",
    "chalk": "^4.1.2",
    "chrono-node": "^2.3.8",
    "dotenv": "^16.0.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.set": "^4.3.2",
    "lodash.unset": "^4.5.2",
    "luxon": "^2.3.1",
    "nano": "^9.0.5",
    "parse-duration": "^1.0.2",
    "pluralize": "^8.0.0",
    "relaxed-json": "^1.0.3",
    "string.ify": "^1.0.64",
    "table": "^6.8.0",
    "tmp-promise": "^3.0.3",
    "yargs": "^17.4.0"
  }
}
