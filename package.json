{
  "name": "datum",
  "version": "2.0.0",
  "description": "Quickly add data to couch db",
  "repository": "git@github.com:vividn/datum.git",
  "author": "Nate Faber <nathaniel@njfaber.com>",
  "license": "UNLICENSED",
  "main": "build/index.js",
  "bin": {
    "dtm": "./build/index.js"
  },
  "scripts": {
    "test": "jest",
    "testwatch": "jest --watch",
    "type": "tsc -p tsconfig.withTests.json",
    "typewatch": "tsc -p tsconfig.withTests.json -w",
    "build": "tsc -p .",
    "buildwatch": "tsc -p . -w",
    "system": "yarn global add $PWD",
    "pipeline": "yarn install && yarn test && yarn build && yarn system",
    "refresh": "rm -rf ./node_modules && yarn install",
    "lint": "prettier src test --check && eslint src test",
    "fmt": "prettier src test --write && eslint src test --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@jest/globals": "^26.4.2",
    "@types/lodash.set": "^4.3.6",
    "@types/lodash.unset": "^4.5.6",
    "@types/luxon": "^1.25.0",
    "@types/nano": "^7.0.0",
    "@types/node": "^14.14.37",
    "@types/pluralize": "^0.0.29",
    "@types/relaxed-json": "^1.0.0",
    "@types/tmp": "^0.2.0",
    "@types/yargs": "^15.0.9",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-jest": "^26.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "timezone-mock": "^1.1.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@types/lodash.get": "^4.4.6",
    "chalk": "^4.1.0",
    "chrono-node": "^2.1.8",
    "dotenv": "^8.2.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "lodash.unset": "^4.5.2",
    "luxon": "^1.25.0",
    "nano": "^8.2.2",
    "pluralize": "^8.0.0",
    "relaxed-json": "^1.0.3",
    "string.ify": "^1.0.64",
    "tmp-promise": "^3.0.2",
    "yargs": "^15.4.1"
  }
}
