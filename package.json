{
  "name": "datum",
  "version": "2.0.0",
  "description": "Quickly add data to couch db",
  "repository": "git@github.com:vividn/datum.git",
  "author": "Nate Faber <nathaniel@njfaber.com>",
  "license": "UNLICENSED",
  "main": "build/index.js",
  "bin": {
    "dtm": "./build/index.js"
  },
  "scripts": {
    "test": "jest",
    "testwatch": "jest --watchAll",
    "type": "tsc -p tsconfig.withTests.json",
    "typewatch": "tsc -p tsconfig.withTests.json -w",
    "build": "rm -rf ./build; tsc -p .",
    "buildwatch": "tsc -p . -w",
    "system": "NODE_ENV=production yarn global add $PWD",
    "pipeline": "yarn refresh && yarn test --maxWorkers 1 && yarn build && rm -rf ./node_modules && yarn install --production && yarn system",
    "refresh": "rm -rf ./node_modules && yarn install",
    "lint": "prettier src test --check && eslint src test",
    "fmt": "prettier src test --write && eslint src test --fix",
    "postinstall": "yarn cache clean datum"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@jest/globals": "^27.2.1",
    "@types/jest": "^27.0.2",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/lodash.set": "^4.3.6",
    "@types/lodash.unset": "^4.5.6",
    "@types/luxon": "^2.0.4",
    "@types/nano": "^7.0.0",
    "@types/node": "^14.14.37",
    "@types/pluralize": "^0.0.29",
    "@types/relaxed-json": "^1.0.1",
    "@types/tmp": "^0.2.1",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-jest": "^27.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^1.1.4",
    "jest": "^26.6.3",
    "jest-mock-extended": "^2.0.2-beta2",
    "prettier": "^2.4.1",
    "timezone-mock": "^1.2.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@types/lodash.get": "^4.4.6",
    "chalk": "^4.1.2",
    "chrono-node": "^2.3.1",
    "dotenv": "^10.0.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.set": "^4.3.2",
    "lodash.unset": "^4.5.2",
    "luxon": "^2.0.2",
    "nano": "^9.0.5",
    "pluralize": "^8.0.0",
    "relaxed-json": "^1.0.3",
    "string.ify": "^1.0.64",
    "tmp-promise": "^3.0.2",
    "yargs": "^17.0.1"
  }
}
